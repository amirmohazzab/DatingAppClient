{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Angular in Chrome",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/src/*",
        "webpack:///./src/*": "${webRoot}/src/*",
        "webpack:///*": "*"
      }
    }
  ]
}